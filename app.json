{
  "expo": {
    "name": "MalNutrix",
    "slug": "MalNutrix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/malnutrix.ic.launcher.dark.png",
    "scheme": "malnutrix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nxhermano.malnutrix",
      "googleServicesFile": "./google-services.json"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/images/malnutrix.ic.launcher.dark.png",
        "monochromeImage": "./assets/images/malnutrix.ic.launcher.dark.png",
        "foregroundImage": "./assets/images/malnutrix.ic.launcher.dark.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json",
      "package": "com.nxhermano.malnutrix"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/malnutrix.ic.launcher.light.png",
          "resizeMode": "contain",
          "imageWidth": 300,
          "dark": {
            "image": "./assets/images/malnutrix.ic.launcher.dark.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000",
            "imageWidth": 300
          }
        }
      ],
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c9cc9878-f237-40fa-a029-ccb6267c6923"
      }
    },
    "owner": "nxhermano"
  }
}
